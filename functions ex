#include <stdio.h>
    void show()
    {
        printf("sweety\n");
    }
    void line()
    {
        printf("---\n");
    }
    int main()
    {
        line();
        show();
        show();
     return 0;
    }
#adding two numbers
#include <stdio.h>
    void add(int x,int y)
    {
        int r=x+y;
        printf("%d",r);
    }
    int main()
    {
        int a,b;
        scanf("%d%d",&a,&b);
        add(a,b);
    }
