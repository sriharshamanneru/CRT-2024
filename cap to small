#include <stdio.h>
#include<string.h>
int main() 
{ 
    char str[100];
    scanf("%s",&str);
    int i,alp=0,dig=0,sym=0;
    for(i=0;str[i]!='\0';i++)
    {
        if(str[i]>=65&&str[i]<=90)
        {
            str[i]=str[i]+32;
        }
        else if(str[i]>=97&&str[i]<=122)
        {
            str[i]=str[i]-32;
        }
    }
    printf("%s",str);
    return 0;
}
